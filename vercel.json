{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/sensors", "dest": "/sensors.html" },
    { "src": "/sensors/(.*)", "dest": "/sensors.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
